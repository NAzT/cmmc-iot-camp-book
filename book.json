{
    "plugins": [
        "gist",
        "ga",
        "image-captions",
        "json",
        "video",
        "-highlight",
        "layout",
        "page-footer"
    ],
    "pluginsConfig": {
        "ga": {
            "token": "UA-XXXX-Y"
        },
        "image-captions": {
            "caption": "_CAPTION_"
        },
        "page-footer": {
            "isShowIssues": false,
            "wisdom": "Made with love by CMMC",
            "copyright": "Copyright &#169; Chiang Mai Maker Club",
            "style": "normal",
            "repo": "cmmakerclub/cmmc-iot-camp-book",
            "isShowQRCode": false,
            "timeColor": "#666",
            "utcOffset": "7",
            "format": "yyyy-MM-dd hh:mm:ss",
            "signature": "Chiang Mai Maker Club",
            "token": "",
            "baseUri": "https://cmmc.io",
            "copyrightColor": "#666",
            "description": "modified at"
        },
        "layout": {
            "headerPath": "layouts/header.html",
            "footerPath": "layouts/footer.html"
        }
    }
}